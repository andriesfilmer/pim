<div id="contacts" class="row">
<% @contacts.each do |contact| %>
  <%= link_to contact_path(contact), class: "small-6 medium-3 large-2 text-center" do %>
    <img src="/assets/cake-filled.svg" class="icon warning"><br>
    <%= contact.name %><br>
    <%= contact.birthdate %>
    (<%= contact.age(contact.birthdate) %>)
  <% end %>
<% end %>
</div>
